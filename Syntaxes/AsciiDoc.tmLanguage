			<string>constant.character.asciidoc</string>
		</dict>
		<key>escape</key>
		<dict>
			<key>match</key>
			<string>\\[-`*_#+.!(){}\[\]\\&gt;:]</string>
			<key>name</key>
			<string>constant.character.escape.asciidoc</string>
		</dict>
